{
    "name": "example_server",
    "version": "0.2.1",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
      "test": "jest --coverage",
      "test:watch": "jest --coverage --watchAll",
      "build": "tsc",
      "build:watch": "onchange 'src/**/*.ts' -- npm run build",
      "view:coverage": "serve coverage/lcov-report",
      "clean": "rm -rf coverage src/**/*.js src/**/*.map",
      "dev": "ts-node src/index.ts",
      "start": "node dist/index.js",
      "importdata": "ts-node src/import.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js --config src/config/db.ts",
      "tags": "node tools/tags.js",
      "subnamespace": "node tools/subnamespace.js",
      "subdomain": "node tools/subdomain.js"
    },
    "devDependencies": {
      "@types/body-parser": "^1.19.0",
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.2",
      "@types/faker": "^4.1.9",
      "@types/jest": "^25.1.2",
      "@types/jsonpath": "^0.2.0",
      "@types/node": "^13.7.1",
      "@types/source-map-support": "^0.5.1",
      "faker": "^4.1.0",
      "jest": "^25.1.0",
      "jsonpath": "^1.0.2",
      "onchange": "^6.1.0",
      "serve": "^11.3.0",
      "source-map-support": "^0.5.16",
      "ts-jest": "^25.2.0",
      "ts-node": "^8.6.2",
      "tsconfig-paths": "^3.9.0",
      "typescript": "3.9.5"
    },
    "dependencies": {
      "@nestjs/common": "7.1.3",
      "@nestjs/core": "7.1.3",
      "@nestjs/platform-express": "7.1.3",
      "@nestjs/swagger": "4.5.11",
      "@nestjs/typeorm": "7.1.0",
      "body-parser": "^1.19.0",
      "class-transformer": "0.2.3",
      "class-validator": "0.12.2",
      "reflect-metadata": "0.1.13",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "pg": "^8.0.0",
      "swagger-ui-express": "4.1.4",
      "tslib": "1.11.1",
      "typeorm": "0.2.25"
    }
  }